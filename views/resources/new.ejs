<div class="container">
  <div class="row">
    <div class="col-md-8">
      <h1>Create a post</h1>
      <form action="/studentresource" method="POST" role="form">      
      	<div class="form-group">
      		<label for="title">Title</label>
      		<input type="text" class="form-control" id="title" name="title" placeholder="Title here">
      	</div>      
      	<div class="form-group">
      		<label for="body">Body</label>
      		<textarea class="form-control" id="body" name="body" rows="15" placeholder="content..."></textarea> 
      	</div>
      	<button type="submit" class="btn btn-primary">Submit</button>
      </form>
    </div>
  </div>
</div>
