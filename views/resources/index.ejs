<div class="jumbotron resource-page-hero">
	<div class="container">
		<h1>Student Resources</h1>
		<p>A curation of resources to assist students. </p>
		<a href="/studentresource/new" class="btn btn-primary">Add resource</a>
	</div>
</div>
<div class="container">
		<% resources.reverse() %>
	  <% _.each(resources, function(resource) { %>
	  <article>
	  	<h2><a href="/studentresource/<%= resource.id %>"><%= resource.title %></a></h2>
	  	<p><%= resource.body.substring(0,300)  %> ... <b></strong><em>read more</em></b></p>
	  </article>
	  <% }) %>
</div>